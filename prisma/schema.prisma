// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Artist {
  id       String  @default(cuid()) @id
  time     DateTime @default(now())
  term     String
  artistId String
  albums   String?
}


